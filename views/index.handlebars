<div class="row">
  <div class="col s12">
    <div class="row">
      {{#each reviewList}}
      <div class="col s6 m3">
        <a href="/review/{{this._id}}">
          <div class="card">
            <div class="card-image">
              <img src="{{imageURL}}">
              <span class="card-title">
                <p>{{this.title}}</p>{{this.writer}}{{this.category}}</span>
            </div>
          </div>
        </a>
      </div>
      {{/each}}
    </div>
  </div>

</div>