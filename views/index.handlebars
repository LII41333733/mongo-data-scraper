<h4><a href="/" class="waves-effect waves-light btn">Scrape Data!</a></h4>
<div class="row article-row">
  <div class="col s12 big-div">
    <div class="row">
      {{#each reviewList}}
      {{#if this.title}}
      <div class="col s6 m4 nfl-article">
        <a href="/review/{{this._id}}">
          <div class="card">
            <div class="card-image">
              <img src="{{imageURL}}">
              <span class="card-title">
                <p><b>{{this.title}}</b></p>
                <p>{{this.writer}}</p>
                </span>
            </div>
          </div>
        </a>
      </div>
      {{/if}}
      {{/each}}
    </div>
  </div>
</div>